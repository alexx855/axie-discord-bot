{
  "name": "axie-discord-bot",
  "version": "2.0.1",
  "author": {
    "name": "alexx855",
    "email": "hello@alexpedersen.dev"
  },
  "description": "A Discord bot that interacts with the Axie Infinity marketplace on the Ronin Network, capable of performing transactions using a private key.",
  "keywords": [
    "axie",
    "discord",
    "bot",
    "web3",
    "marketplace",
    "ronin",
    "roninchain",
    "viem",
    "axieinfinity",
    "axie-infinity",
    "discord-bot",
    "ronin-network",
    "marketplace-transactions",
    "blockchain",
    "crypto-gaming",
    "web3-gaming",
    "nft",
    "nft-marketplace",
    "private-key-transactions"
  ],
  "license": "MIT",
  "engines": {
    "node": ">=20.x"
  },
  "scripts": {
    "start": "nodemon src/index.ts",
    "build": "rimraf dist && tsc",
    "deploy": "vercel --prod",
    "install-commands": "npx ts-node src/install-commands.ts",
    "ts.check": "tsc --project tsconfig.json",
    "eslint": "npx eslint src/. --ext .ts --fix"
  },
  "devDependencies": {
    "@types/express": "^4.17.14",
    "@types/node": "^18.11.3",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "dotenv": "^16.3.1",
    "eslint": "^8.57.0",
    "eslint-config-standard-with-typescript": "^43.0.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-promise": "^6.1.1",
    "nodemon": "^3.0.2",
    "rimraf": "^5.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^5.4.2"
  },
  "dependencies": {
    "@axieinfinity/mixer": "^1.4.0-beta.14",
    "@roninbuilders/contracts": "^0.3.3",
    "@vercel/node": "^3.0.11",
    "discord-interactions": "^3.2.0",
    "express": "^4.18.2",
    "viem": "^1.19.11"
  },
  "ts-node": {
    "transpileOnly": true
  }
}
